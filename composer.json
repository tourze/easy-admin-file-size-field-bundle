{
  "name": "tourze/easy-admin-file-size-field-bundle",
  "description": "A Symfony bundle that provides a file size field for EasyAdmin, automatically formatting byte values into human-readable formats",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "chrisullyott/php-filesize": "^4.2",
    "easycorp/easyadmin-bundle": "^4",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/expression-language": "^7.3",
    "symfony/form": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/options-resolver": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/string": "^7.3",
    "symfony/twig-bundle": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "0.0.*",
    "twig/twig": "^3.21"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\EasyAdminFileSizeFieldBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\EasyAdminFileSizeFieldBundle\\Tests\\": "tests"
    }
  }
}